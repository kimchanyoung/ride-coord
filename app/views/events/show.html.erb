<%= @event.name %>
<%= link_to "Add a vehicle", new_event_vehicle_path(@event) %>
<%= link_to "Add a user", new_event_user_path(@event) %>
<%= 'Rideless Folks' %>
<% @event.rideless_users.each do |user| %>
  <% user.fname %>
<% end %>
<% @event.vehicles.each do |vehicle| %>
  <%= vehicle.name %>
  <% vehicle.users.each do |user|%>
    <%= user.fname %>
  <% end %>
<% end %>